{
  "name": "@gpuexchange/meta-agent",
  "productName": "GX Meta Agent",
  "version": "0.2.0",
  "description": "META Enhanced Trading Agent",
  "bin": "build/core.js",
  "scripts": {
    "start": "cross-env ENV=debug node src/core/index.js",
    "test": "jest src/ --no-cache",
    "coverage": "jest src/ --coverage  --coverageReporters=text-lcov",
    "coveralls": "npm run coverage | coveralls",
    "lint": "standard src/",
    "lint-fix": "standard src/ --fix",
    "dev": "webpack --mode development",
    "watch": "webpack --watch --progress --hide-modules --mode development",
    "hot": "webpack-dev-server --inline --hot",
    "production": "webpack --progress --hide-modules --mode production",
    "package": "npm run production && pkg package.json --out-path bin --targets node8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gpuexchange/meta-agent.git"
  },
  "keywords": [
    "gpu",
    "exchange",
    "crypto",
    "mining",
    "trading",
    "agent"
  ],
  "author": "GPU Exchange <hello@gpu.exchange>",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://bitbucket.org/gpuexchange/meta-agent#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.3.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.4",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "nyc": "^11.8.0",
    "sass-loader": "^7.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "isomorphic-fetch": "^2.2.1",
    "object-path": "^0.11.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "serve-index": "^1.9.1",
    "webpack-cli": "^2.1.3"
  },
  "sideEffecs": false,
  "jest": {
    "modulePathIgnorePatterns": [
      "lib/"
    ]
  },
  "pkg": {
    "assets": "build/**/*"
  }
}
