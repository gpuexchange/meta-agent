{
  "name": "@gpuexchange/meta-agent",
  "productName": "GX Meta Agent",
  "version": "0.2.0",
  "description": "META Enhanced Trading Agent",
  "main": "src/index.js",
  "bin": "build/index.bundle.js",
  "scripts": {
    "lint": "eslint src/ --fix",
    "test": "jest src/ --no-cache",
    "build": "npm run build-core && npm run build-frontend",
    "build-core": "ENV=server babel src/core/index.js -o build/core.bundle.js",
    "build-frontend": "ENV=client babel src/frontend/index.js -o build/frontend.bundle.js",
    "package": "npm run build && pkg package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/gpuexchange/meta-agent.git"
  },
  "keywords": [
    "gpu",
    "exchange",
    "crypto",
    "mining",
    "trading",
    "agent"
  ],
  "author": "GPU Exchange <hello@gpu.exchange>",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://bitbucket.org/gpuexchange/meta-agent#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "electron-packager": "^10.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.8.2",
    "webpack": "^4.0"
  },
  "dependencies": {
    "object-path": "^0.11.4"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "lib/"
    ]
  }
}
